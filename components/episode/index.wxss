.container {
  height: 60rpx;
  width: fit-content;
  font-size: 34rpx;
  color: #333;
  display: flex;
  align-items: center;
  justify-content: center;
  text-shadow: 0 0 2rpx #ccc;
}

.no {
  font-size: 60rpx;
  font-weight: bold;
}

.line {
  height: 50rpx;
  width: 4rpx;
  margin: 0 8rpx;
  background-color: #333;
}

.date {
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.date text {
  line-height: 30rpx;
}
