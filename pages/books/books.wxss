.container {
  padding-top: 100rpx;
  background-color: #f5f5f5;
}

.searchBar {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  width: 100%;
}
.titleWrap {
  width: 750rpx;
  display: flex;
  justify-content: center;
}

.title {
  color: #666;
  font-weight: bold;
  margin-bottom: 30rpx;
  border-bottom: 6rpx solid palegoldenrod;
}

.books {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}
